{
  "name": "ReactJS Chrome Extension",
  "version": "1.0",
  "description": "Chrome extension using ReactJS",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "React Extension",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background/background.js"
    ],
    "persistent": true
  }
}