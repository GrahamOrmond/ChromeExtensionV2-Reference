{
  "name": "OAuth Tutorial",
  "key": "",
  "version": "1.0",
  "description": "Uses OAuth to get access tokens and make api calls",
  "manifest_version": 2,
  "permissions": [
      "storage",
      "identity",
      "webRequest",
      "*://localhost:*/"
  ],
  "browser_action": {
    "default_title": "OAuth Tutorial",
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": [
      "background/background.js"
    ],
    "persistent": true
  }
}